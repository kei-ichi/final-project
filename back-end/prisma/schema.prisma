// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:admin@localhost:3306/user"
}

model User {
  id   String @id @default(uuid())
  name String
  strength_1 String
  strength_2 String
  strength_3 String
  strength_4 String
  strength_5 String
}
